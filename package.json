{
  "name": "unified-typings-test",
  "version": "0.0.1",
  "description": "",
  "main": "dist/src/index.js",
  "author": "Hernan Rajchert <hrajchert@gmail.com>",
  "license": "MIT",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/hrajchert/unified-typings-test"
  },
  "scripts": {
    "start": "tsns run",
    "test": "run-s build-and:test",
    "tdd": "run-p watch:*",
    "build": "tsns build",
    "docker:build": "docker build -t ${npm_package_name}:${npm_package_version} .",
    "docker:run": "docker run --rm ${npm_package_name}:${npm_package_version}",
    "commitmsg": "validate-commit-msg",
    "precommit": "run-s lint:* \"build -- --no-unused-locals\"",
    "prepush": "run-s build-and:test",
    "lint:ts": "tslint --project tsconfig.json",
    "lint-and-fix:-ts": "tslint --project tsconfig.json --fix",
    "build-and:test": "run-s build test-once",
    "build-and:start": "run-s build start",
    "watch:ts": "tsc -w",
    "watch:test": "mocha dist/**/*.test.js -w --colors --inspect --growl",
    "test-once": "mocha dist/**/*.test.js --inspect --colors",
    "contributors-add": "all-contributors add",
    "contributors-generate": "all-contributors generate",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "dependencies": {
    "@types/react": "^16.4.18",
    "@types/vfile": "^2.2.2",
    "react": "^16.6.0",
    "rehype-document": "^2.2.0",
    "rehype-format": "^2.3.0",
    "rehype-stringify": "^4.0.0",
    "remark-parse": "^6.0.1",
    "remark-rehype": "^3.0.1",
    "tsns": "0.0.18",
    "typescript": "3.0.1",
    "vfile-reporter": "^5.1.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.4",
    "@types/mocha": "5.2.5",
    "@types/node": "10.5.7",
    "all-contributors-cli": "5.4.0",
    "chai": "4.1.2",
    "docz": "^0.12.7",
    "husky": "0.14.3",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "sinon": "6.1.5",
    "tslint": "5.11.0",
    "tslint-config-acamica": "2.0.0",
    "validate-commit-msg": "2.14.0"
  }
}
